{
	"manifest_version": 2,
	"name": "Live Weather Report",
	"version": "0.1",
	"description": "Get Live Weather Report",
	"permissions": ["tabs", "geolocation"],
	"icons": { "16": "icon16.png",
           		"48": "icon48.png",
          		"128": "icon128.png" },
	"browser_action": {
		"default_icon": "icon48.png",
		"default_popup":"popup.html"
	}
}
